package com.example.methods;

public class Main {

	public static void main(String[] args) {

		int num= 2;
		
		num= num*2;
		num= num+3;
		System.out.println(num);
		
		num= num*2;
		num= num+3;
		System.out.println(num);
		
		num= num*2;
		num= num+3;
		System.out.println(num);
		
		num= num*2;
		num= num+3;
		System.out.println(num);
	}

}
