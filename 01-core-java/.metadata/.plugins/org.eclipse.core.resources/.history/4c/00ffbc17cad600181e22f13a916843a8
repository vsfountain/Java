package com.example.primitives;

public class PrimExample {
	
	public static void main(String[] args) {
		
		int x;	//this is how we declare a variable in java
		int x2= 16; //this is how we declare a variable AND assign it a value;
		
		/*
		 * What are the primitives types in java???
		 * 
		 * int (4 bytes), float (8 bytes), short (2 bytes), double (8 bytes),
		 * long (8 bytes), boolean (1 bit), byte (8 bits), char (2 bytes), 
		 */
		
	}
}
