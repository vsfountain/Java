import org.springframework.stereotype.Component;

import com.project2.daodesignpattern.service.UpdateServiceImplementation;

@Component("aspects")
@Aspect
public class AspectService {
	final static Logger logger = Logger.getLogger(UpdateServiceImplementation.class);

	public AspectService() {
		
	}
	@Before("execution(* *Login*(..))")
public void  Login(JoinPoint jp) {
	
}
}
